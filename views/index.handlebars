<h2>Lista de Productos</h2>
<ul>
    {{#each products}}
        <li>
            <strong>{{this.title}}</strong> - ${{this.price}}
            {{#if this.description}}
                <p>{{this.description}}</p>
            {{/if}}
        </li>
    {{else}}
        <li>No hay productos disponibles.</li>
    {{/each}}
</ul>
